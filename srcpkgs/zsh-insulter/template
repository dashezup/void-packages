# Template file for 'zsh-insulter'
pkgname=zsh-insulter
version=0.1
revision=1
build_style=fetch
short_desc="Insults the user when typing wrong command"
maintainer="Dash Eclipse <dash@ezup.dev>"
license="Unknown"
homepage="https://github.com/matmutant/${pkgname}"

do_install() {
	$XBPS_FETCH_CMD https://raw.githubusercontent.com/matmutant/zsh-insulter/master/src/zsh.command-not-found
	vinstall zsh.command-not-found 644 etc
}
